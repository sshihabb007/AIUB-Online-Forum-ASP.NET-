@using Forum.Entity
@model IEnumerable<Forum.MVC.UI.Models.Users>

<html>

<head>
</head>

<body>
    <fieldset>
        <legend>Students List</legend>
        <table align="center" id="StudentTable" width="100%" border="1" cellspacing="0" border="1" cellpadding="5" class="table table-striped table-bordered table-hover">
            <thead>
                <tr class="w3-green">
                    <th>ID</th>
                    <th>Name</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody id="">
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        <td><a href="/Moderator/StudentProfile/@item.Id">View</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </fieldset>
    <script>
        $(document).ready(function () {
            $('#StudentTable').dataTable({
                "bProcessing": true,
                "bInfo": false
            });
            $('.teachertable_length').addClass('bs-select');
        });
    </script>
</body>
</html>


