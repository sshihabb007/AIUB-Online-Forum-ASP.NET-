@model IEnumerable<Forum.MVC.UI.Models.Users>
<html>

<head>
</head>

<body>
    <table align="center" id="manageposttable" width="100%" border="1" cellspacing="0" border="1" cellpadding="5" class="table table-striped table-bordered table-hover">
        <thead>
            <tr class="w3-green">
                <th>ID</th>
                <th>Name</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody id="ManagePost">
            @foreach (var item in Model)
            {
                if (item.Status != Forum.Entity.UserStatus.Blocked && item.Status != Forum.Entity.UserStatus.Pending)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        <td><a href="/Moderator/PostListOfUser/@item.Id">View Post List</a></td>
                    </tr>
                }
            }
        </tbody>
    </table>
    <script>
        $(document).ready(function () {
            $('#manageposttable').dataTable({
                "bProcessing": true,
                "bInfo": false
            });
        });
    </script>
</body>
</html>


